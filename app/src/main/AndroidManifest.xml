<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.smartepay">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppWithTransparentStatusBar">
        <activity android:name=".guest.QRCodeGenertorActivity"></activity>
        <activity android:name=".guest.CartActivity" />
        <activity android:name=".guest.MarketItemsActivity" />
        <activity android:name=".shop.ItemsActivity" />
        <activity android:name=".shop.CategoriesActivity" />
        <activity android:name=".shop.BrandsActivity" />
        <activity android:name=".shop.MainShopActivity" />
        <activity android:name=".shop.ShopRegisterActivity" />
        <activity android:name=".shop.ShopLoginActivity" />
        <activity android:name=".guest.MainGuestActivity" />
        <activity android:name=".StartActivity" />
        <activity
            android:name=".SplashActivity"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>